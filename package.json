{
  "name": "@isa/react",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "modules/*",
    "packages/*",
    "server/api/*",
    "server/*"
  ],
  "scripts": {
    "start:gut": "turbo run start:gut",
    "start:harea": "turbo run start:harea",
    "start:bo-local": "turbo run start:bo-local",
    "start:bo-harea": "turbo run start:bo-harea",
    "start:bo-harea-local": "turbo run start:bo-harea",
    "build": "turbo run build",
    "build:server": "turbo run build:server",
    "build:bo-harea": "turbo run build:bo-harea",
    "build:harea": "turbo run build:harea",
    "deploy:harea": "yarn run build:server && yarn run build:bo-harea && yarn run build:harea",
    "dev": "turbo run dev --parallel",
    "dev:bo": "turbo run dev:bo",
    "dev:bo-local": "turbo run dev:bo-local",
    "lint": "turbo run lint",
    "test": "jest",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.180",
    "@types/node": "^14.14.36",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^2.1.0",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.4.12",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "esbuild": "^0.14.11",
    "esbuild-node-externals": "^1.4.1",
    "esbuild-sass-plugin": "^2.0.3",
    "esbuild-style-plugin": "^1.6.0",
    "eslint-config-custom": "*",
    "history": "^5.0.0",
    "jsonic": "^1.0.1",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.16",
    "prettier": "latest",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "ttypescript": "^1.5.13",
    "turbo": "latest",
    "typescript": "^4.2.3",
    "typescript-transform-paths": "^3.3.1",
    "uuid": "^8.3.2",
    "vite": "^3.1.0",
    "vite-plugin-svgr": "^2.2.1",
    "vite-tsconfig-paths": "^3.5.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@tailwindcss/line-clamp": "^0.4.2",
    "@types/uuid": "^8.3.4",
    "avatar-initials": "^6.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "postcss": "^8.4.17",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^11.8.5",
    "react-query": "^3.8.2",
    "react-router-dom": "^6.0.0",
    "react-scripts": "^5.0.1",
    "recoil": "^0.7.5",
    "smoothscroll-polyfill": "^0.4.4",
    "tailwindcss": "^3.1.8",
    "web-vitals": "^1.0.1"
  },
  "packageManager": "yarn@1.22.18"
}
